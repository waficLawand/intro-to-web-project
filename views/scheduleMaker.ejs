function timeSort(day)
{
  var hold;
  for(var i =0;i<day.length;i++)
  {
    for(var j=0;j<(day.length)-i-1;j++)
    {

      if(parseInt(day[j].CRN.from) >parseInt(day[j+1].CRN.from))
      {
        hold = day[j];
        day[j]=day[j+1];
        day[j+1]=hold;
      }
    }
  }
}
